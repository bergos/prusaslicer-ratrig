{
  "name": "prusaslicer-ratrig",
  "version": "0.0.0",
  "description": "RatRig PrusaSlicer settings",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "prusaslicer-util ini --default-filaments=RATRIG --sort-properties src/*.ini > dist/RatRig.ini",
    "copy": "cp -R dist/*",
    "update": "cp dist/RatRig.ini $HOME/.config/PrusaSlicer/vendor/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bergos/prusaslicer-ratrig.git"
  },
  "keywords": [
    "prusa",
    "ratrig",
    "slicer"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bergos/prusaslicer-ratrig/issues"
  },
  "homepage": "https://github.com/bergos/prusaslicer-ratrig",
  "dependencies": {
  },
  "devDependencies": {
    "prusaslicer-util": "git://github.com/bergos/prusaslicer-util.git#master"
  }
}
